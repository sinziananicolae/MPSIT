<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Beehive Monitor</title>
    @Styles.Render("~/Content/bootstrap-css")
    @Styles.Render("~/Content/font-awesome-css")
    @Styles.Render("~/Content/jquery-ui-css")
    @Styles.Render("~/Content/toastr-css")
    @Styles.Render("~/Content/theme-css")
    @Styles.Render("~/Content/app-css")

</head>
<body>
    @RenderBody()

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/lodash")
    @Scripts.Render("~/bundles/momentjs")
    @Scripts.Render("~/bundles/chartjs")
    @Scripts.Render("~/bundles/angular")
    @Scripts.Render("~/bundles/jquery-ui")
    @Scripts.Render("~/bundles/bootstrap-ui")
    @Scripts.Render("~/bundles/toastr")
    @Scripts.Render("~/bundles/underscore")
    <script src='//maps.googleapis.com/maps/api/js?sensor=false'></script>

    @RenderSection("scripts", required: false)
</body>
</html>
